{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./bin",
    "target": "es5",
    "jsx": "react",
    "rootDir": "./src",
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "lib": ["es2016", "dom", "es5"]
  },
  "include": [
    "./src/**/*"
  ]
}
